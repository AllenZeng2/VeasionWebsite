<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.veasion.mapper.IpRecordMapper" >
  <resultMap id="BaseResultMap" type="cn.veasion.entity.IpRecord" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="ip" property="ip" jdbcType="VARCHAR" />
    <result column="date" property="date" jdbcType="TIMESTAMP" />
    <result column="line" property="line" jdbcType="INTEGER" />
    <result column="area" property="area" jdbcType="VARCHAR" />
  </resultMap>
  
  <sql id="Base_Column_List" >
    id, ip, date, line, area
  </sql>
  
  <!-- 分页查询 -->
  <select id="select" resultMap="BaseResultMap" parameterType="cn.veasion.bean.PageModel">
  	select ip.* from ip_record ip where 1=1 
  	<include refid="pageModelWhere" />
  	<if test="orderBy != null">
  		order by #{orderBy} ${isDesc ? "desc" : "asc"}
  	</if>
  	limit #{limitStart}, #{limitEnd}
  </select>
  
  <select id="count" resultType="int" parameterType="cn.veasion.bean.PageModel">
  	select count(*) from ip_record ip where 1=1 
  	<include refid="pageModelWhere" />
  </select>
  
  <sql id="pageModelWhere">
  	<if test="obj != null">
  		<if test="obj.id != null">
  			and ip.id=#{obj.id}
  		</if>
  		<if test="obj.ip != null">
  			and ip.ip like CONCAT('%',#{obj.ip},'%')
  		</if>
  		<if test="obj.area != null">
  			and ip.area like CONCAT('%',#{obj.area},'%')
  		</if>
  	</if>
  </sql>
  
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select 
    <include refid="Base_Column_List" />
    from ip_record
    where id = #{id,jdbcType=INTEGER}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    delete from ip_record
    where id = #{id,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="cn.veasion.entity.IpRecord" >
    insert into ip_record (id, ip, date, 
      line, area)
    values (#{id,jdbcType=INTEGER}, #{ip,jdbcType=VARCHAR}, #{date,jdbcType=TIMESTAMP}, 
      #{line,jdbcType=INTEGER}, #{area,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="cn.veasion.entity.IpRecord" >
    insert into ip_record
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="ip != null" >
        ip,
      </if>
      <if test="date != null" >
        date,
      </if>
      <if test="line != null" >
        line,
      </if>
      <if test="area != null" >
        area,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="ip != null" >
        #{ip,jdbcType=VARCHAR},
      </if>
      <if test="date != null" >
        #{date,jdbcType=TIMESTAMP},
      </if>
      <if test="line != null" >
        #{line,jdbcType=INTEGER},
      </if>
      <if test="area != null" >
        #{area,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="cn.veasion.entity.IpRecord" >
    update ip_record
    <set >
      <if test="ip != null" >
        ip = #{ip,jdbcType=VARCHAR},
      </if>
      <if test="date != null" >
        date = #{date,jdbcType=TIMESTAMP},
      </if>
      <if test="line != null" >
        line = #{line,jdbcType=INTEGER},
      </if>
      <if test="area != null" >
        area = #{area,jdbcType=VARCHAR},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  <update id="updateByPrimaryKey" parameterType="cn.veasion.entity.IpRecord" >
    update ip_record
    set ip = #{ip,jdbcType=VARCHAR},
      date = #{date,jdbcType=TIMESTAMP},
      line = #{line,jdbcType=INTEGER},
      area = #{area,jdbcType=VARCHAR}
    where id = #{id,jdbcType=INTEGER}
  </update>
</mapper>